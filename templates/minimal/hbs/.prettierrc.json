{

  "singleQuote": true,
  "trailingComma": "es5",
  "printWidth": 80,
  "plugins": ["prettier-plugin-packagejson"],
    "packagejson.order": [
    "name",
    "version",
    "description",
    "bin",
    "keywords",
    "homepage",
    "bugs",
    "repository",
    "license",
    "author",
    "contributors",
    "files",
    "main",
    "module",
    "types",
    "scripts",
    "config",
    "dependencies",
    "devDependencies",
    "peerDependencies",
    "optionalDependencies",
    "engines",
    "os",
    "cpu",
    "private",
    "publishConfig"
  ],
    "packagejson": {
        "sortScripts": "alphabetical"
    },
    "packagejsonGroups": {
        "scripts": ["start", "build", "test", "docs", "precommit", "format"]
    }
}